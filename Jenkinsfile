pipeline {
    agent any

    stages {
        stage('Clone') {
            steps {
                echo 'Cloning the repository...'
               
            }
        }

        stage('Build') {
            steps {
                echo 'Compiling the code...'
                
            }
        }

        stage('Run') {
            steps {
                echo 'Running the program...'
                
            }
        }
    }

}
